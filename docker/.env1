# General Polling Settings
POLLER_TYPE=finnhub                        # Poller type (e.g., finnhub, polygon, yfinance, etc.)
POLL_INTERVAL=60                            # Polling interval in seconds
REQUEST_TIMEOUT=30                          # Timeout for API requests in seconds
MAX_RETRIES=3                               # Max retries for failed requests
RETRY_DELAY=5                               # Delay between retries in seconds
LOG_LEVEL=INFO                              # Log level (e.g., INFO, DEBUG, WARNING)

# API Keys for Pollers
POLYGON_API_KEY=your-polygon-api-key       # API Key for Polygon.io
FINNHUB_API_KEY=your-finnhub-api-key       # API Key for Finnhub
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key # API Key for Alpha Vantage
YFINANCE_API_KEY=your-yfinance-api-key     # API Key for Yahoo Finance (if needed)
IEX_API_KEY=your-iex-api-key               # API Key for IEX Cloud
QUANDL_API_KEY=your-quandl-api-key         # API Key for Quandl

# RabbitMQ Cluster Configuration
RABBITMQ_DEFAULT_USER=rabbitmq_user        # RabbitMQ default user
RABBITMQ_DEFAULT_PASS=rabbitmq_pass        # RabbitMQ default password
RABBITMQ_DEFAULT_VHOST=your_vhost          # RabbitMQ virtual host (vhost)
RABBITMQ_ERLANG_COOKIE=secret_cookie       # Erlang cookie for clustering
RABBITMQ_CLUSTER_NODES=rabbit@rabbitmq     # RabbitMQ cluster nodes (use the correct name here)

# RabbitMQ Queue Names (can define up to 10 queues)
QUEUE_1=stock_data_1                       # Queue 1 name
QUEUE_2=stock_data_2                       # Queue 2 name
QUEUE_3=stock_data_3                       # Queue 3 name
QUEUE_4=stock_data_4                       # Queue 4 name
QUEUE_5=stock_data_5                       # Queue 5 name
QUEUE_6=stock_data_6                       # Queue 6 name
QUEUE_7=stock_data_7                       # Queue 7 name
QUEUE_8=stock_data_8                       # Queue 8 name
QUEUE_9=stock_data_9                       # Queue 9 name
QUEUE_10=stock_data_10                     # Queue 10 name

# Poller Specific Settings (for each poller)
FINNHUB_API_KEY=your-finnhub-api-key       # Finnhub API key
POLYGON_API_KEY=your-polygon-api-key       # Polygon.io API key
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key # Alpha Vantage API key
YFINANCE_API_KEY=your-yfinance-api-key     # Yahoo Finance API key (if needed)
IEX_API_KEY=your-iex-api-key               # IEX Cloud API key
QUANDL_API_KEY=your-quandl-api-key         # Quandl API key

# Stock Poller Configuration (e.g., Finnhub, Polygon, AlphaVantage)
FILL_RATE_LIMIT=60                         # Max API calls per minute (depending on the API)
POLL_INTERVAL=60                            # Interval between polling requests in seconds

# Logging Configuration
LOG_LEVEL=INFO                             # The logging level (INFO, DEBUG, etc.)

# AWS Settings
AWS_ACCESS_KEY_ID=your-access-key-id       # AWS Access Key for AWS SDKs (e.g., Boto3)
AWS_SECRET_ACCESS_KEY=your-secret-access-key # AWS Secret Access Key
AWS_REGION=us-east-1                        # AWS Region for SQS, Lambda, etc.

# Queue Type (choose between RabbitMQ and SQS)
QUEUE_TYPE=rabbitmq                        # Could also be 'sqs' for AWS SQS
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/your-queue # AWS SQS Queue URL

# Timeout and Retry Settings for Pollers
REQUEST_TIMEOUT=30                         # Request timeout in seconds
MAX_RETRIES=3                              # Max retries for API requests
RETRY_DELAY=5                              # Delay between retries in seconds

# Additional Environment Variables for Poller Behavior
POLL_TIMEOUT=10                            # Poller timeout in seconds (for each API call)
MAX_API_CALLS_PER_MIN=5                    # Max API calls per minute (to prevent rate limiting)
ENABLE_RETRY=true                          # Enable retries for failed requests (true/false)
RETRY_INTERVAL=10                          # Retry interval in seconds

# RabbitMQ Configuration (Advanced Options)
RABBITMQ_URL=amqp://rabbitmq_user:rabbitmq_pass@rabbitmq:5672/  # RabbitMQ URL for connection
RABBITMQ_EXCHANGE=stock_exchange         # Exchange name for RabbitMQ
RABBITMQ_ROUTING_KEY=stock_data         # Default routing key for RabbitMQ
RABBITMQ_QUEUES=stock_data_1,stock_data_2,stock_data_3,stock_data_4,stock_data_5,stock_data_6,stock_data_7,stock_data_8,stock_data_9,stock_data_10  # Queue names

# Enable/Disable Features
ENABLE_METRICS=true                        # Enable metrics tracking (true/false)
ENABLE_VALIDATION=true                     # Enable validation of data before sending (true/false)

# SQS Configuration (If Using SQS Instead of RabbitMQ)
SQS_REGION=us-east-1                        # AWS region for SQS queue
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/your-queue # SQS Queue URL

# Cloud Logging Configuration (Optional)
CLOUD_LOGGING_ENABLED=false                # Enable logging to a cloud provider (AWS CloudWatch, GCP, etc.)
CLOUD_LOGGING_ENDPOINT=https://logs.us-east-1.amazonaws.com  # Example endpoint for AWS CloudWatch

# Enable Pollers (Specify which pollers should be enabled)
POLLERS=alphavantage,finnhub,iex,polygon,quandl,yfinance  # A comma-separated list of active pollers to use (e.g., "finnhub,polygon")

# Other Settings
ENABLE_BACKFILL=true                       # Enable backfilling of historical data (true/false)
POLL_TIMEOUT=30                            # Timeout for each API request (in seconds)
MAX_API_CALLS_PER_MIN=5                    # Max API calls per minute (to prevent rate limiting)
ENABLE_RETRY=true                          # Enable retries for failed requests (true/false)
RETRY_INTERVAL=10                          # Retry interval in seconds